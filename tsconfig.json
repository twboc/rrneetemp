{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "_version": "2.0.3",
  "compilerOptions": {
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "strictNullChecks": true,
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": false,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["app/index.d.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "webpack",
    "electron/main.js",
    "index.js",
    "app/postcss.config.js",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}