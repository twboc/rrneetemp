{
  "name": "rrneew-boilerplate",
  "version": "3.0.2",
  "description": "React + React Native + Electron + Express + Webpack Boilderplate",
  "homepage": "https://www.project.com",
  "main": "electron/main.js",
  "author": {
    "name": "Barney Rubble",
    "email": "b@rubble.com",
    "url": "http://barnyrubble.tumblr.com/"
  },
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "native": "react-native start",
    "test": "jest",
    "electron": "electron .",
    "start": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.app.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.app.prod.js",
    "server": "cross-env NODE_ENV=production webpack --config webpack/webpack.server.prod.js",
    "buildweb": "yarn run build && yarn run server",
    "lint": "eslint 'app/**/*.tsx' || true",
    "prettify": "prettier --write 'app/**/*.tsx'",
    "dist:linux": "electron-builder --linux"
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate",
    "react",
    "react native",
    "electron"
  ],
  "build": {
    "appId": "com.example.app",
    "directories": {
      "output": "releases/${platform}/${arch}"
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "rpm"
      ],
      "category": "Utility"
    }
  },
  "repository": {},
  "dependencies": {
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.5",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-router-dom": "^6.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "electron": "^23.2.1",
    "electron-builder": "^23.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "*",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
