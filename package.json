{
  "name": "rrneew-boilerplate",
  "version": "3.0.2",
  "description": "React + React Native + Electron + Express + Webpack Boilderplate",
  "homepage": "https://www.project.com",
  "main": "electron/main.js",
  "author": {
    "name": "Mysterious Stranger",
    "email": "ms@stranger.com",
    "url": "http://mysteriousstranger.com/"
  },
  "license": "MIT",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "electron": "electron .",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.app.dev.js",
    "site": "cross-env NODE_ENV=production webpack --config webpack/webpack.site.prod.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.app.prod.js",
    "server": "cross-env NODE_ENV=production webpack --config webpack/webpack.server.prod.js",
    "serve": "yarn run server && yarn run listen",
    "listen": "node ./dist/server/server.js",
    "deploy": "yarn run web && yarn run listen",
    "frontend": "yarn run site && yarn run build",
    "web": "yarn run site && yarn run build && yarn run server",
    "lint": "eslint 'app/**/*.tsx' || true",
    "prettify": "prettier --write 'app/**/*.tsx'",
    "dist:linux": "electron-builder --linux",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "webpack",
    "webpack 5",
    "webpack boilerplate",
    "react",
    "react native",
    "electron"
  ],
  "build": {
    "appId": "com.example.app",
    "directories": {
      "output": "releases/${platform}/${arch}"
    },
    "linux": {
      "target": [
        "deb",
        "snap",
        "rpm"
      ],
      "category": "Utility"
    }
  },
  "repository": {},
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@prisma/client": "^4.13.0",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "add": "^2.0.6",
    "axios": "^1.3.5",
    "bootstrap": "^5.2.3",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "font-awesome": "^4.7.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.0",
    "native-base": "^3.4.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.71.5",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "12.1.1",
    "react-native-vector-icons": "^9.2.0",
    "react-router-dom": "^6.10.0",
    "store": "^2.0.12",
    "ts-node": "^10.9.1",
    "uuid": "^9.0.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@types/cookie-parser": "^1.4.3",
    "@types/jest": "^29.5.0",
    "@types/js-cookie": "^3.0.3",
    "@types/jsdom": "^21.1.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/store": "^2.0.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "electron": "^23.2.1",
    "electron-builder": "^23.6.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.8",
    "prisma": "4.12.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "*",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
